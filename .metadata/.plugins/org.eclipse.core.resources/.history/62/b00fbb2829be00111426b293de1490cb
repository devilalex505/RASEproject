#include "DeplacementManuel.h"
#include "Servo.h"
#include "Arduino.h"
#include "HardwareSerial.h"

void deplacementM(Servo serv, int pos, int val){



				if(val == 1)
				{
					pos += 3;
					serv.write(pos);
				}
				if(val == 2)
				{
					pos -= 3;
					serv.write(pos);
				}



}
